{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 32
    },
    "password": {
      "type": "string",
      "maxLength": 32
    },
    "isAdmin": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "password",
    "isAdmin"
  ]
}
